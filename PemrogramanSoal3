#include <iostream>
#include <string>
using namespace std;

string cekWarna (int detik) {
    int posisi = (detik - 25) % 103;

    if (posisi < 20) return "Hijau";
    else if (posisi < 23) return "Kuning";
    else return "Merah";
}

int main() {
    int waktu[] = {80, 135, 150, 212};

    for (int i = 0; i < 4; i++) {
        cout << "Detik ke-" << waktu[i] << ": " << cekWarna (waktu[i]) << endl;
    }

    return 0;
}
